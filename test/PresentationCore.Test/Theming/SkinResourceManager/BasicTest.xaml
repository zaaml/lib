<zm:ThemeSkinResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
  xmlns:zm="http://schemas.zaaml.com/xaml">

  <!--  BaseValues  -->
  <zm:SkinDictionary x:Key="BaseValues">

    <system:Int32 x:Key="Value1">1</system:Int32>
    <system:Int32 x:Key="Value2">2</system:Int32>

    <!--  NestedValues  -->
    <zm:SkinDictionary x:Key="Nested">
      <system:Int32 x:Key="Value1">11</system:Int32>
      <system:Int32 x:Key="Value2">12</system:Int32>
    </zm:SkinDictionary>

  </zm:SkinDictionary>

  <!--  FinalValues  -->
  <zm:SkinDictionary x:Key="FinalValues" BasedOn="BaseValues">

    <system:Int32 x:Key="Value2">3</system:Int32>

    <!--  NestedValues  -->
    <zm:SkinDictionary x:Key="Nested">
      <system:Int32 x:Key="Value1">14</system:Int32>
    </zm:SkinDictionary>

  </zm:SkinDictionary>

</zm:ThemeSkinResourceDictionary>
<zm:ThemeResourceDictionary
    xmlns:s="clr-namespace:System;assembly=mscorlib"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:zm="http://schemas.zaaml.com/xaml"
    xmlns:zmb="http://schemas.zaaml.com/build"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    mc:Ignorable="zmb">

  <zm:ThemeResourceDictionary.MergedDictionaries>
    <zm:ThemeResourceDictionary Source="../Glyphs.Shared.xaml" />
    <zm:ThemeResourceDictionary Source="./ThemeKeywordAssets.xaml" />
  </zm:ThemeResourceDictionary.MergedDictionaries>

  <!-- CheckBox -->
  <zm:SkinDictionary x:Key="Code.Shared.CheckBox">

    <s:String x:Key="UncheckedGlyph">M 0,0 Z</s:String>
    <s:String x:Key="CheckedGlyph">F1 M 0,120L 0,260L 140,400L 400,140L 400,0L 140,260L 0,120 Z</s:String>
    <s:String x:Key="IndeterminateGlyph">F1 M 100,100L 300,100L 300,300L 100,300L 100,100 Z</s:String>

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF000000" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF000000" />
      <SolidColorBrush x:Key="Pressed" Color="#FF000000" />
      <SolidColorBrush x:Key="Checked" Color="#FF000000" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Checked" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Checked" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- RadioButton -->
  <zm:SkinDictionary x:Key="Code.Shared.RadioButton">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF000000" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF000000" />
      <SolidColorBrush x:Key="Pressed" Color="#FF000000" />
      <SolidColorBrush x:Key="Checked" Color="#FF000000" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Checked" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Checked" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- Button -->
  <zm:SkinDictionary x:Key="Code.Shared.Button">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF000000" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF000000" />
      <SolidColorBrush x:Key="Pressed" Color="#FF000000" />
      <SolidColorBrush x:Key="Checked" Color="#FF000000" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFA9A9A9" />
      <SolidColorBrush x:Key="Checked" Color="#FFA9A9A9" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Checked" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- DropDownGlyph -->
  <zm:SkinDictionary x:Key="Code.Shared.DropDownGlyph">

    <!-- Style -->
    <Style x:Key="Style" TargetType="zm:PathIcon">
      <Setter Property="Stretch" Value="None"/>
      <Setter Property="BrushMode" Value="Fill" />
      <Setter Property="StrokeThickness" Value="0" />
    </Style>

    <!-- Data -->
    <zm:ThemeResource x:Key="Data" Value="{StaticResource Code.Glyphs.AngleArrows7Px.Down}" />

  </zm:SkinDictionary>

  <!-- DropDownButton -->
  <zm:SkinDictionary x:Key="Code.Shared.DropDownButton" BasedOn="Code.Shared.Button">

    <!-- Glyph -->
    <zm:SkinDictionary x:Key="Glyph" BasedOn="Code.Shared.DropDownGlyph" />

  </zm:SkinDictionary>

  <!-- SplitButton -->
  <zm:SkinDictionary x:Key="Code.Shared.SplitButton" BasedOn="Code.Shared.Button">

    <!-- Splitter -->
    <zm:SkinDictionary x:Key="Splitter">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="PopupOpened" Color="#FFB9B9B9" />
    </zm:SkinDictionary>

    <!-- DropDownButton -->
    <zm:SkinDictionary x:Key="DropDownButton" BasedOn="Code.Shared.Button">

      <!-- BorderBrush -->
      <zm:SkinDictionary x:Key="BorderBrush">
        <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
        <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      </zm:SkinDictionary>

      <!-- Glyph -->
      <zm:SkinDictionary x:Key="Glyph" BasedOn="Code.Shared.DropDownGlyph" />

    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- ToolBarButton -->
  <zm:SkinDictionary x:Key="Code.Shared.ToolBarButton">

    <zm:SkinDictionary x:Key="Input">
      <Color x:Key="ForegroundBase">#E3000000</Color>
      <Color x:Key="BackgroundBase">#00000000</Color>
      <Color x:Key="BorderBrushBase">#00000000</Color>
      <Color x:Key="AccentBase">#FF3369D5</Color>
    </zm:SkinDictionary>

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <zm:SkinDictionary.Generators>
        <zm:SolidColorBrushGenerator ColorExpression="BaseColor" 
                                     ExpressionScope="BaseColor:$(../Input.ForegroundBase)">

          <zm:SkinResourceTemplate x:Key="Normal" />
          <zm:SkinResourceTemplate x:Key="MouseOver" />
          <zm:SkinResourceTemplate x:Key="Pressed" />
          <zm:SkinResourceTemplate x:Key="Checked" />
        </zm:SolidColorBrushGenerator>
      </zm:SkinDictionary.Generators>
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <zm:SkinDictionary.Generators>
        <zm:SolidColorBrushGenerator ColorExpression="Shade(BaseColor, Shade)"
                                     ExpressionScope="BaseColor:$(../Input.BackgroundBase)">

          <zm:SkinResourceTemplate x:Key="Normal" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0"/>
          <zm:SkinResourceTemplate x:Key="MouseOver" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.2" />
          <zm:SkinResourceTemplate x:Key="Pressed" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.3"/>
          <zm:SkinResourceTemplate x:Key="Checked" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.25"/>
        </zm:SolidColorBrushGenerator>
      </zm:SkinDictionary.Generators>
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <zm:SkinDictionary.Generators>
        <zm:SolidColorBrushGenerator ColorExpression="Shade(BaseColor, Shade)" 
                                     ExpressionScope="BaseColor:$(../Input.BorderBrushBase)">

          <zm:SkinResourceTemplate x:Key="Normal" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0"/>
          <zm:SkinResourceTemplate x:Key="MouseOver" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.2" />
          <zm:SkinResourceTemplate x:Key="Pressed" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.3"/>
          <zm:SkinResourceTemplate x:Key="Checked" zm:SolidColorBrushGenerator.ExpressionScope="Shade:0.25"/>
        </zm:SolidColorBrushGenerator>
      </zm:SkinDictionary.Generators>
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- ToolBarDropDownButton -->
  <zm:SkinDictionary x:Key="Code.Shared.ToolBarDropDownButton" BasedOn="Code.Shared.ToolBarButton">

    <!-- Glyph -->
    <zm:SkinDictionary x:Key="Glyph" BasedOn="Code.Shared.DropDownGlyph" />

  </zm:SkinDictionary>

  <!-- ToolBarSplitButton -->
  <zm:SkinDictionary x:Key="Code.Shared.ToolBarSplitButton" BasedOn="Code.Shared.ToolBarButton">

    <!-- Splitter -->
    <zm:SkinDictionary x:Key="Splitter">
      <SolidColorBrush x:Key="Normal" Color="#FFADADAD" zmb:Skin="Office" />
      <SolidColorBrush x:Key="PopupOpened" Color="#FFAEAEAE" zmb:Skin="Office" />
    </zm:SkinDictionary>

    <!-- DropDownButton -->
    <zm:SkinDictionary x:Key="DropDownButton" BasedOn="Code.Shared.ToolBarDropDownButton">

      <!-- Reference Input -->
      <zm:SkinDictionary x:Key="Input" BasedOn="../Input" />

    </zm:SkinDictionary>

  </zm:SkinDictionary>

  <!-- ToolBar -->
  <zm:SkinDictionary x:Key="Code.Shared.ToolBar">
    <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" zmb:Skin="Office" />

    <!-- MenuButton -->
    <zm:SkinDictionary x:Key="MenuButton" BasedOn="Code.Shared.ToolBarDropDownButton" />
  </zm:SkinDictionary>

  <!-- TabControl -->
  <zm:SkinDictionary x:Key="Code.Shared.TabControl">
    <SolidColorBrush x:Key="Foreground" Color="#FF373A3D" />
    <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BorderBrush" Color="#FFB9B9B9" />
    <SolidColorBrush x:Key="DisableVeilBrush" Color="#8CFFFFFF" />
  </zm:SkinDictionary>

  <!-- TabItem -->
  <zm:SkinDictionary x:Key="Code.Shared.TabItem">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF373A3D" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.ForegroundColor}" />
      <SolidColorBrush x:Key="Selected" Color="#FF3369D5" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BackgroundColor}" />
      <SolidColorBrush x:Key="Selected" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BorderColor}" />
      <SolidColorBrush x:Key="Selected" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- SelectionLine -->
    <zm:SkinDictionary x:Key="SelectionLine">
      <SolidColorBrush x:Key="Selected" Color="#FF3369D5" />
      <SolidColorBrush x:Key="SelectedInactive" Color="#FFB9B9B9" />
    </zm:SkinDictionary>

    <SolidColorBrush x:Key="DisableVeilBrush" Color="#8CFFFFFF" />
  </zm:SkinDictionary>

  <!-- TextBox -->
  <zm:SkinDictionary x:Key="Code.Shared.TextBox">
    <SolidColorBrush x:Key="CaretBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="Watermark.Foreground" Color="#FF6D6D6D" />

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF000000" />
      <SolidColorBrush x:Key="ReadOnly" Color="#FF000000" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF000000" />
      <SolidColorBrush x:Key="Disabled" Color="#FF000000" />
      <SolidColorBrush x:Key="Focused" Color="#FF000000" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="ReadOnly" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="Disabled" Color="#A5F7F7F7" />
      <SolidColorBrush x:Key="Focused" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="ReadOnly" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Disabled" Color="#A5F7F7F7" />
      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- MenuItem -->
  <zm:SkinDictionary x:Key="Code.Shared.MenuItem">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF000000" />
      <SolidColorBrush x:Key="Highlighted" Color="#FF000000" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.ForegroundColor}" />
      <SolidColorBrush x:Key="Pressed" Color="#FF000000" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="Highlighted" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BackgroundColor}" />
      <SolidColorBrush x:Key="Pressed" Color="#FFA9A9A9" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="Highlighted" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BorderColor}" />
      <SolidColorBrush x:Key="Pressed" Color="#FFA9A9A9" />
    </zm:SkinDictionary>

    <SolidColorBrush x:Key="DisableVeilBrush" Color="#8CFFFFFF" />
  </zm:SkinDictionary>

  <!-- Glyph -->
  <Style x:Key="Code.SmallGlyphStyle" TargetType="Path">
    <Setter Property="Width" Value="10" />
    <Setter Property="Height" Value="10" />
    <Setter Property="Fill" Value="#FF606060" />
    <Setter Property="StrokeThickness" Value="0" />
    <Setter Property="Stretch" Value="Fill" />
  </Style>

  <!-- ScrollViewer -->
  <zm:SkinDictionary  x:Key="Code.Shared.ScrollViewer">
    <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BorderBrush" Color="#FFB9B9B9" />
  </zm:SkinDictionary>

  <!-- ScrollBar -->
  <zm:SkinDictionary  x:Key="Code.Shared.ScrollBar">
    <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BorderBrush" Color="#FFFFFFFF" />
  </zm:SkinDictionary>

  <!-- ScrollBar RepeatButton -->
  <zm:SkinDictionary x:Key="Code.Shared.ScrollBar.RepeatButton">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF373A3D" />
      <SolidColorBrush x:Key="Pressed" Color="#FFA9A9A9" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="Pressed" Color="#00FFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#00FFFFFF" />
      <SolidColorBrush x:Key="Pressed" Color="#00FFFFFF" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- ScrollBar Thumb -->
  <zm:SkinDictionary x:Key="Code.Shared.ScrollBar.Thumb">

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF373A3D" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FF373A3D" />
      <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- ScrollBar Glyph -->
  <zm:SkinDictionary x:Key="Code.Shared.ScrollBar.Glyph">

    <!-- Style -->
    <Style x:Key="Style" TargetType="Path">
      <Setter Property="Stretch" Value="None"/>
      <Setter Property="StrokeThickness" Value="0"/>
      <Setter Property="Width" Value="8"/>
      <Setter Property="Height" Value="8"/>
    </Style>

    <!-- Data -->
    <zm:SkinDictionary x:Key="Data">
      <zm:ThemeResource x:Key="Left" Value="{StaticResource Code.Glyphs.Arrows8Px.Left}"/>
      <zm:ThemeResource x:Key="Up" Value="{StaticResource Code.Glyphs.Arrows8Px.Up}"/>
      <zm:ThemeResource x:Key="Right" Value="{StaticResource Code.Glyphs.Arrows8Px.Right}"/>
      <zm:ThemeResource x:Key="Down" Value="{StaticResource Code.Glyphs.Arrows8Px.Down}"/>
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- ListView -->
  <zm:SkinDictionary x:Key="Code.Shared.ListView">
    <SolidColorBrush x:Key="Foreground" Color="#FF373A3D" />
    <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="BorderBrush" Color="#FFB9B9B9" />
    <Thickness x:Key="ScrollViewerMargin">0</Thickness>
    <Thickness x:Key="ScrollViewerPadding">1</Thickness>
  </zm:SkinDictionary>

  <!-- ListViewItem -->
  <zm:SkinDictionary x:Key="Code.Shared.ListViewItem">

    <!-- Foreground -->
    <zm:SkinDictionary x:Key="Foreground">
      <SolidColorBrush x:Key="Normal" Color="#FF373A3D" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.ForegroundColor}" />
      <SolidColorBrush x:Key="Disabled" Color="#FF373A3D" />

      <SolidColorBrush x:Key="Focused" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="Unfocused" Color="#FF373A3D" />

      <SolidColorBrush x:Key="Selected" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="Unselected" Color="#FF373A3D" />
      <SolidColorBrush x:Key="SelectedUnfocused" Color="#FF373A3D" />
    </zm:SkinDictionary>

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BackgroundColor}" />
      <SolidColorBrush x:Key="Disabled" Color="#00FFFFFF" />

      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
      <SolidColorBrush x:Key="Unfocused" Color="#FFFFFFFF" />

      <SolidColorBrush x:Key="Selected" Color="#FF3369D5" />
      <SolidColorBrush x:Key="Unselected" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="SelectedUnfocused" Color="#FFB8B8B8" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="{StaticResource Code.ControlItem.MouseOver.LowContrast.BorderColor}" />
      <SolidColorBrush x:Key="Disabled" Color="#FFB9B9B9" />

      <SolidColorBrush x:Key="Focused" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Unfocused" Color="#FFB9B9B9" />

      <SolidColorBrush x:Key="Selected" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Unselected" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="SelectedUnfocused" Color="#FFB9B9B9" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

  <!-- DropDownItemsControl -->
  <zm:SkinDictionary x:Key="Code.Shared.DropDownItemsControl">

    <!-- Background -->
    <zm:SkinDictionary x:Key="Background">
      <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="ReadOnly" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFFFFFFF" />
      <SolidColorBrush x:Key="Disabled" Color="#A5F7F7F7" />
      <SolidColorBrush x:Key="Focused" Color="#FFFFFFFF" />
    </zm:SkinDictionary>

    <!-- BorderBrush -->
    <zm:SkinDictionary x:Key="BorderBrush">
      <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="ReadOnly" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" />
      <SolidColorBrush x:Key="Disabled" Color="#A5F7F7F7" />
      <SolidColorBrush x:Key="Focused" Color="#FF3369D5" />
    </zm:SkinDictionary>
  </zm:SkinDictionary>

</zm:ThemeResourceDictionary>
<zm:ThemeSkinResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="clr-namespace:System;assembly=mscorlib"
    xmlns:zm="http://schemas.zaaml.com/xaml"
    xmlns:zmb="http://schemas.zaaml.com/build"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    mc:Ignorable="zmb"
    Name="Office">

  <zm:ThemeSkinResourceDictionary.MergedDictionaries>
    <zm:ThemeResourceDictionary Source="/Zaaml.Themes.Metro;component/Skins/Glyphs.Shared.xaml" />
  </zm:ThemeSkinResourceDictionary.MergedDictionaries>

  <zm:SkinDictionary x:Key="Metro">

    <!-- ContentPresenter -->
    <zm:SkinDictionary x:Key="ContentPresenter" BasedOn="Metro.Shared.ContentPresenter">
      <SolidColorBrush x:Key="Foreground" Color="#FF000000" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- CheckBoxGlyphControl -->
    <zm:SkinDictionary x:Key="CheckBoxGlyphControl" BasedOn="Metro.Shared.CheckBox">
      <SolidColorBrush x:Key="ReadOnlyVeilBackground" Color="#77FFFFFF" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- RadioGlyphControl -->
    <zm:SkinDictionary x:Key="RadioGlyphControl"  BasedOn="Metro.Shared.RadioButton">
      <SolidColorBrush x:Key="ReadOnlyVeilBackground" Color="#77FFFFFF" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- Label -->
    <zm:SkinDictionary x:Key="Label">
      <SolidColorBrush x:Key="Foreground" Color="#FF000000" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- ToolBarTray-->
    <zm:SkinDictionary x:Key="ToolBarTray">
      <SolidColorBrush x:Key="Background" Color="#00000000" zmb:Skin="Office" />
    </zm:SkinDictionary>

    <!-- ToolBarControl -->
    <zm:SkinDictionary x:Key="ToolBarControl" BasedOn="Metro.Shared.ToolBar">
      <SolidColorBrush x:Key="Background" Color="#00000000" />

      <!-- MenuButton -->
      <zm:SkinDictionary x:Key="MenuButton" BasedOn="Metro.ToolBarDropDownButton" />

      <!-- ToolBarButton -->
      <zm:SkinDictionary x:Key="ToolBarButton" BasedOn="Metro.ToolBarButton" />

      <!-- ToolBarDropDownButton -->
      <zm:SkinDictionary x:Key="ToolBarDropDownButton" BasedOn="Metro.ToolBarDropDownButton" />

      <!-- ToolBarSplitButton -->
      <zm:SkinDictionary x:Key="ToolBarSplitButton" BasedOn="Metro.ToolBarSplitButton" />

      <!-- ToolBarSeparator -->
      <zm:SkinDictionary x:Key="ToolBarSeparator" BasedOn="Metro.ToolBarSeparator" />

    </zm:SkinDictionary>

    <!-- AppToolBarControl -->
    <zm:SkinDictionary x:Key="AppToolBarControl" BasedOn="Metro.Shared.ToolBar">

      <SolidColorBrush x:Key="Background" Color="#FF2576C4" />

      <!-- BaseColors -->
      <zm:SkinDictionary x:Key="BaseColors">

        <zm:SkinDictionary x:Key="Input">
          <Color x:Key="ForegroundBase">#FFFFFFFF</Color>
          <Color x:Key="BackgroundBase">#00FFFFFF</Color>
          <Color x:Key="BorderBrushBase">#00FFFFFF</Color>
          <Color x:Key="AccentBase">#FF0072C6</Color>
        </zm:SkinDictionary>

      </zm:SkinDictionary>

      <!-- MenuButton -->
      <zm:SkinDictionary x:Key="MenuButton" BasedOn="Metro.ToolBarDropDownButton | Metro.AppToolBarControl.BaseColors" />

      <!-- ToolBarButton -->
      <zm:SkinDictionary x:Key="ToolBarButton" BasedOn="Metro.ToolBarButton | Metro.AppToolBarControl.BaseColors" />

      <!-- ToolBarDropDownButton -->
      <zm:SkinDictionary x:Key="ToolBarDropDownButton" BasedOn="Metro.ToolBarDropDownButton | Metro.AppToolBarControl.BaseColors" />

      <!-- ToolBarSplitButton -->
      <zm:SkinDictionary x:Key="ToolBarSplitButton" BasedOn="Metro.ToolBarSplitButton | Metro.AppToolBarControl.BaseColors" />

      <!-- ToolBarSeparator -->
      <zm:SkinDictionary x:Key="ToolBarSeparator" BasedOn="Metro.ToolBarSeparator" />

    </zm:SkinDictionary>

    <!-- ToolBarButton -->
    <zm:SkinDictionary x:Key="ToolBarButton" BasedOn="Metro.Shared.ToolBarButton" />

    <!-- ToolBarDropDownButton -->
    <zm:SkinDictionary x:Key="ToolBarDropDownButton" BasedOn="Metro.Shared.ToolBarDropDownButton" />

    <!-- ToolBarSplitButton -->
    <zm:SkinDictionary x:Key="ToolBarSplitButton" BasedOn="Metro.Shared.ToolBarSplitButton" />

    <!-- ToolBarSeparator -->
    <zm:SkinDictionary x:Key="ToolBarSeparator">

      <SolidColorBrush x:Key="SeparatorBrush" Color="#FFB9B9B9" zmb:Skin="Light" />

    </zm:SkinDictionary>

    <!-- SeparatorControl -->
    <zm:SkinDictionary x:Key="SeparatorControl">
      <SolidColorBrush x:Key="Stroke" Color="#FFB9B9B9" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- DisableVeilControl -->
    <zm:SkinDictionary x:Key="DisableVeilControl">
      <SolidColorBrush x:Key="Background" Color="#7FE6E6E6" zmb:Skin="Office" />
    </zm:SkinDictionary>

    <!-- ValidationErrorControl -->
    <zm:SkinDictionary x:Key="ValidationErrorControl">
      <SolidColorBrush x:Key="Foreground" Color="#FFFFFFFF" zmb:Skin="Light" />
      <SolidColorBrush x:Key="Background" Color="#FFDC000C" zmb:Skin="Light" />
      <SolidColorBrush x:Key="BorderBrush" Color="#FFDC000C" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- HyperlinkButton -->
    <zm:SkinDictionary x:Key="HyperlinkButton">

      <!-- Foreground -->
      <zm:SkinDictionary x:Key="Foreground">
        <SolidColorBrush x:Key="Normal" Color="#FF0066CC" zmb:Skin="Light" />
        <SolidColorBrush x:Key="Disabled" Color="Gray" />
        <SolidColorBrush x:Key="MouseOver" Color="Red" />
        <SolidColorBrush x:Key="Pressed" Color="#FF0066CC" zmb:Skin="Light" />
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- WindowFramePresenter -->
    <zm:SkinDictionary x:Key="WindowFramePresenter">

      <!-- Background -->
      <zm:SkinDictionary x:Key="Background">
        <SolidColorBrush x:Key="Normal" Color="#FFE6E6E6" zmb:Skin="Office" />
        <SolidColorBrush x:Key="Active" Color="#FFE6E6E6" zmb:Skin="Office" />
      </zm:SkinDictionary>

      <!-- BorderBrush -->
      <zm:SkinDictionary x:Key="BorderBrush">
        <SolidColorBrush x:Key="Normal" Color="#FFB9B9B9" zmb:Skin="Office" />
        <SolidColorBrush x:Key="Active" Color="#FFB9B9B9" zmb:Skin="Office" />
      </zm:SkinDictionary>

      <Color x:Key="Shadow">#FF000000</Color>
    </zm:SkinDictionary>

    <!-- WindowPresenter -->
    <zm:SkinDictionary x:Key="WindowPresenter">

      <!-- WindowFramePresenter -->
      <zm:SkinDictionary x:Key="WindowFramePresenter" BasedOn="Metro.WindowFramePresenter" />

    </zm:SkinDictionary>

    <!-- Window -->
    <zm:SkinDictionary x:Key="Window">

      <SolidColorBrush x:Key="DisableVeilBrush" Color="#8CE6E6E6" zmb:Skin="Office" />

      <!-- WindowPresenter -->
      <zm:SkinDictionary x:Key="WindowPresenter" BasedOn="Metro.WindowPresenter" />

      <!-- Header -->
      <zm:SkinDictionary x:Key="Header">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#E3000000" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Active" Color="#E3000000" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#FFE6E6E6" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Active" Color="#FFE6E6E6" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Default Button-->
        <zm:SkinDictionary x:Key="DefaultButton">

          <!-- Foreground -->
          <zm:SkinDictionary x:Key="Foreground">
            <SolidColorBrush x:Key="Normal" Color="#FF6A6A6A" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFFEFEFE" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- Background -->
          <zm:SkinDictionary x:Key="Background">
            <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FF3665B2" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFC65050" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- BorderBrush -->
          <zm:SkinDictionary x:Key="BorderBrush">
            <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FF3665B2" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFC65050" zmb:Skin="Office" />
          </zm:SkinDictionary>
        </zm:SkinDictionary>

        <!-- Close Button-->
        <zm:SkinDictionary x:Key="CloseButton">

          <!-- Foreground -->
          <zm:SkinDictionary x:Key="Foreground">
            <SolidColorBrush x:Key="Normal" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFFEFEFE" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- Background -->
          <zm:SkinDictionary x:Key="Background">
            <SolidColorBrush x:Key="Normal" Color="#FFDF4343" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFC65050" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFDF4343" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- BorderBrush -->
          <zm:SkinDictionary x:Key="BorderBrush">
            <SolidColorBrush x:Key="Normal" Color="#FFDF4343" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFC65050" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFDF4343" zmb:Skin="Office" />
          </zm:SkinDictionary>
        </zm:SkinDictionary>

        <!-- Buttons -->
        <zm:SkinDictionary x:Key="Buttons">

          <Thickness x:Key="Margin">5,-1,5,8</Thickness>

          <!-- Minimize -->
          <zm:SkinDictionary x:Key="Minimize"  BasedOn="Metro.Window.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.Office.Window.Header.MinimizeButton}"/>
          </zm:SkinDictionary>

          <!-- Maximize -->
          <zm:SkinDictionary x:Key="Maximize"  BasedOn="Metro.Window.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.Office.Window.Header.MaximizeButton}"/>
          </zm:SkinDictionary>

          <!-- Restore -->
          <zm:SkinDictionary x:Key="Restore"  BasedOn="Metro.Window.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.Office.Window.Header.RestoreButton}"/>
          </zm:SkinDictionary>

          <!-- Close -->
          <zm:SkinDictionary x:Key="Close" BasedOn="Metro.Window.Header.CloseButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.Office.Window.Header.CloseButton}"/>
          </zm:SkinDictionary>
        </zm:SkinDictionary>
      </zm:SkinDictionary>

      <!-- Footer -->
      <zm:SkinDictionary x:Key="Footer">

        <!-- Buttons -->
        <zm:SkinDictionary x:Key="Buttons">
          <Thickness x:Key="Padding">10,0,10,10</Thickness>
        </zm:SkinDictionary>
      </zm:SkinDictionary>

      <!-- ButtonsSplitter -->
      <zm:SkinDictionary x:Key="ButtonsSplitter">
        <SolidColorBrush x:Key="Stroke" Color="#00FFFFFF" zmb:Skin="Office" />
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- AppWindow -->
    <zm:SkinDictionary x:Key="AppWindow">

      <SolidColorBrush x:Key="DisableVeilBrush" Color="#8CE6E6E6" zmb:Skin="Office" />

      <!-- WindowPresenter -->
      <zm:SkinDictionary x:Key="WindowPresenter">

        <!-- WindowFramePresenter -->
        <zm:SkinDictionary x:Key="WindowFramePresenter">

          <!-- Background -->
          <zm:SkinDictionary x:Key="Background">
            <SolidColorBrush x:Key="Normal" Color="#FFE6E6E6" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Active" Color="#FFE6E6E6" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- BorderBrush -->
          <zm:SkinDictionary x:Key="BorderBrush">
            <SolidColorBrush x:Key="Normal" Color="#FF0173C7" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Active" Color="#FF0173C7" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <Color x:Key="Shadow">#FF000000</Color>
        </zm:SkinDictionary>

      </zm:SkinDictionary>

      <!-- Header -->
      <zm:SkinDictionary x:Key="Header">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#FFFFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Active" Color="#FFFFFFFF" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#FF2576C4" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Active" Color="#FF2576C4" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Default Button-->
        <zm:SkinDictionary x:Key="DefaultButton">

          <!-- Foreground -->
          <zm:SkinDictionary x:Key="Foreground">
            <SolidColorBrush x:Key="Normal" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFFEFEFE" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- Background -->
          <zm:SkinDictionary x:Key="Background">
            <SolidColorBrush x:Key="Normal" Color="#00000000" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#40000000" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#30000000" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- BorderBrush -->
          <zm:SkinDictionary x:Key="BorderBrush">
            <SolidColorBrush x:Key="Normal" Color="#00000000" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#40000000" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#30000000" zmb:Skin="Office" />
          </zm:SkinDictionary>
        </zm:SkinDictionary>

        <!-- Close Button-->
        <zm:SkinDictionary x:Key="CloseButton">

          <!-- Foreground -->
          <zm:SkinDictionary x:Key="Foreground">
            <SolidColorBrush x:Key="Normal" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFFEFEFE" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFFEFEFE" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- Background -->
          <zm:SkinDictionary x:Key="Background">
            <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFC65050" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFDF4343" zmb:Skin="Office" />
          </zm:SkinDictionary>

          <!-- BorderBrush -->
          <zm:SkinDictionary x:Key="BorderBrush">
            <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
            <SolidColorBrush x:Key="MouseOver" Color="#FFC65050" zmb:Skin="Office" />
            <SolidColorBrush x:Key="Pressed" Color="#FFDF4343" zmb:Skin="Office" />
          </zm:SkinDictionary>
        </zm:SkinDictionary>

        <!-- Buttons -->
        <zm:SkinDictionary x:Key="Buttons">

          <Thickness x:Key="Margin">5,-1,-1,0</Thickness>

          <!-- Minimize -->
          <zm:SkinDictionary x:Key="Minimize"  BasedOn="Metro.AppWindow.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.AppWindow.Header.MinimizeButton}"/>
          </zm:SkinDictionary>

          <!-- Maximize -->
          <zm:SkinDictionary x:Key="Maximize"  BasedOn="Metro.AppWindow.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.AppWindow.Header.MaximizeButton}"/>
          </zm:SkinDictionary>

          <!-- Restore -->
          <zm:SkinDictionary x:Key="Restore"  BasedOn="Metro.AppWindow.Header.DefaultButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.AppWindow.Header.RestoreButton}"/>
          </zm:SkinDictionary>

          <!-- Close -->
          <zm:SkinDictionary x:Key="Close" BasedOn="Metro.AppWindow.Header.CloseButton">
            <zm:ThemeResource x:Key="Data" Value="{StaticResource Metro.Glyphs.AppWindow.Header.CloseButton}"/>
          </zm:SkinDictionary>
        </zm:SkinDictionary>
      </zm:SkinDictionary>

      <!-- Footer -->
      <zm:SkinDictionary x:Key="Footer">

        <!-- Buttons -->
        <zm:SkinDictionary x:Key="Buttons">
          <Thickness x:Key="Padding">10,0,10,10</Thickness>
        </zm:SkinDictionary>
      </zm:SkinDictionary>

      <!-- ButtonsSplitter -->
      <zm:SkinDictionary x:Key="ButtonsSplitter">
        <SolidColorBrush x:Key="Stroke" Color="#00FFFFFF" zmb:Skin="Office" />
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- MessageWindow -->
    <zm:SkinDictionary x:Key="MessageWindow">
      <SolidColorBrush x:Key="Foreground" Color="#E3000000" zmb:Skin="Office" />
    </zm:SkinDictionary>

    <!-- ContextMenu -->
    <zm:SkinDictionary x:Key="ContextMenu">
      <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" zmb:Skin="Light" />
      <SolidColorBrush x:Key="BorderBrush" Color="#FFADADAD" zmb:Skin="Light" />
      <SolidColorBrush x:Key="SeparatorStroke" Color="#FFADADAD" zmb:Skin="Light" />
    </zm:SkinDictionary>

    <!-- MenuItem -->
    <zm:SkinDictionary x:Key="MenuItem" BasedOn="Metro.Shared.MenuItem">

      <!-- Icon -->
      <zm:SkinDictionary x:Key="Icon">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- BorderBrush -->
        <zm:SkinDictionary x:Key="BorderBrush">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Office" />
        </zm:SkinDictionary>
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- ToggleMenuItem -->
    <zm:SkinDictionary x:Key="ToggleMenuItem" BasedOn="Metro.Shared.MenuItem">

      <!-- Icon -->
      <zm:SkinDictionary x:Key="Icon">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#FFB8B8B8" zmb:Skin="Office" />
        </zm:SkinDictionary>

        <!-- BorderBrush -->
        <zm:SkinDictionary x:Key="BorderBrush">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
          <SolidColorBrush x:Key="Checked" Color="#FFB8B8B8" zmb:Skin="Office" />
        </zm:SkinDictionary>
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- CheckBoxMenuItem -->
    <zm:SkinDictionary x:Key="CheckBoxMenuItem" BasedOn="Metro.Shared.MenuItem">

      <!-- Icon -->
      <zm:SkinDictionary x:Key="Icon">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#FF000000" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#FF000000" zmb:Skin="Light" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Light" />
        </zm:SkinDictionary>

        <!-- BorderBrush -->
        <zm:SkinDictionary x:Key="BorderBrush">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Light" />
        </zm:SkinDictionary>
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- RadioMenuItem -->
    <zm:SkinDictionary x:Key="RadioMenuItem" BasedOn="Metro.Shared.MenuItem">

      <!-- Icon -->
      <zm:SkinDictionary x:Key="Icon">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#FF000000" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#FF000000" zmb:Skin="Light" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Light" />
        </zm:SkinDictionary>

        <!-- BorderBrush -->
        <zm:SkinDictionary x:Key="BorderBrush">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Checked" Color="#00FFFFFF" zmb:Skin="Light" />
        </zm:SkinDictionary>
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- Menu -->
    <zm:SkinDictionary x:Key="Menu">
      <SolidColorBrush x:Key="Background" Color="#FFFFFFFF" zmb:Skin="Light" />

      <!-- ToggleButton -->
      <zm:SkinDictionary x:Key="ToggleButton">
        <SolidColorBrush x:Key="BackgroundNormal" Color="Transparent" />
        <SolidColorBrush x:Key="BackgroundHover" Color="#FFB9B9B9" zmb:Skin="Light" />
        <SolidColorBrush x:Key="BackgroundPressed" Color="#FFB9B9B9" zmb:Skin="Light" />
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- MenuBar-->
    <zm:SkinDictionary x:Key="MenuBar">
      <SolidColorBrush x:Key="Background" Color="#FFE6E6E6" zmb:Skin="Office" />
    </zm:SkinDictionary>

    <!-- MenuScrollViewer -->
    <zm:SkinDictionary x:Key="MenuScrollViewer">

      <!-- Button -->
      <zm:SkinDictionary x:Key="Button">

        <!-- Foreground -->
        <zm:SkinDictionary x:Key="Foreground">
          <SolidColorBrush x:Key="Normal" Color="#FF000000" zmb:Skin="Light" />
          <SolidColorBrush x:Key="MouseOver" Color="#FF000000" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Pressed" Color="#FF000000" zmb:Skin="Light" />
        </zm:SkinDictionary>

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Light" />
          <SolidColorBrush x:Key="MouseOver" Color="#FFB9B9B9" zmb:Skin="Light" />
          <SolidColorBrush x:Key="Pressed" Color="#FFB9B9B9" zmb:Skin="Light" />
        </zm:SkinDictionary>
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- TabViewControl -->
    <zm:SkinDictionary x:Key="TabViewControl" BasedOn="Metro.Shared.TabControl" />

    <!-- TabViewItemsPresenter -->
    <zm:SkinDictionary x:Key="TabViewItemsPresenter">

      <!-- ItemsMenuButton -->
      <zm:SkinDictionary x:Key="ItemsMenuButton" BasedOn="Metro.DropDownButton">
        <SolidColorBrush x:Key="Background.Normal" Color="#00FFFFFF" />
      </zm:SkinDictionary>
    </zm:SkinDictionary>

    <!-- TabViewItem -->
    <zm:SkinDictionary x:Key="TabViewItem" BasedOn="Metro.Shared.TabItem">

      <!-- Button -->
      <zm:SkinDictionary x:Key="Button">

        <!-- Background -->
        <zm:SkinDictionary x:Key="Background">
          <SolidColorBrush x:Key="Normal" Color="#00FFFFFF" zmb:Skin="Office" />
